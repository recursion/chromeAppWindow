if [ $# -eq 0 ]; then
  /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --app=http://cryptowat.ch
elif [ $1 -eq 1 ]; then
  /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --app=http://netflix.com
else
  /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --app=$1
fi
